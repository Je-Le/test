{
  "version": 1,
  "name": "OSM OpenMapTiles",
  "id": "openmaptiles",
  "sources": {
    "maplibre-satellite": {
        "tiles": [
            "https://tile.openstreetmap.org/{z}/{x}/{y}.png"
        ]
    }
  },
  "glyphs": "{fontstack}/{range}.pbf",
  "sprite": "sprite",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "background-color": "#f2efe9"
      }
    }
  ]
}
